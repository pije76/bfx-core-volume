<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts-config SYSTEM "struts-config.dtd">

<struts-config>


    <!-- ================================================ Action Mappings =============================================== -->
    <action-mappings>
        <mapping name="home"  path="/"  action="HomeAction">
            <forward name="home"  include=".home"/>
        </mapping>
        
        <mapping name="bonkers"  path="/Paypal/TFV/index.php"  action="BonkersAction">
        </mapping>
    </action-mappings>


    <!-- ===================================================== Tiles ==================================================== -->
    <tiles>
        <tile name=".home"  file="home.phtml"/>
    </tiles>
</struts-config>
